\name{fCalcAVPfromVMFandPress}
\alias{fCalcAVPfromVMFandPress}
\title{fCalcAVPfromVMFandPress}
\description{Calculate AVP from VMF and Press}
\usage{fCalcAVPfromVMFandPress(VMF = VMF.V.n, Press = Press.V.n, 
    VMF.V.n, Press.V.n)}
\arguments{
  \item{VMF}{Vapor mole fraction (VMF, mol / mol)}
  \item{Press}{Atmospheric pressure (Press, hPa)}
  \item{VMF.V.n}{deprecated}
  \item{Press.V.n}{deprecated}
}

\value{Data vector of actual vapor pressure (AVP, hPa (mbar))}

